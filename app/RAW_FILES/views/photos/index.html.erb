<p id="notice"><%= notice %></p>

<h1>Listing Photos</h1>

<table>
  <thead>
    <tr>
      <th>Source</th>
      <th>Wt ht</th>
      <th>Title</th>
      <th>Caption</th>
      <th>Loc taken</th>
      <th>Date taken</th>
      <th>Rating</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.source %></td>
        <td><%= photo.wt_ht %></td>
        <td><%= photo.title %></td>
        <td><%= photo.caption %></td>
        <td><%= photo.loc_taken %></td>
        <td><%= photo.date_taken %></td>
        <td><%= photo.rating %></td>
        <td><%= photo.user %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
